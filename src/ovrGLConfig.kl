/// Contains platform-specific information for rendering.
struct ovrGLConfig
{
  ovrRenderAPIType API;
  ovrSize RTSize;
  Integer Multisample;
  /// The optional window handle. If unset, rendering will use the current window.
  UInt64 Window;
};
